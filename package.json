{
  "name": "gatsby-starter-default",
  "private": true,
  "description": "A simple starter to get up and developing quickly with Gatsby",
  "version": "0.1.0",
  "author": "Kyle Mathews <mathews.kyle@gmail.com>",
  "dependencies": {
    "@mdx-js/mdx": "^0.20.3",
    "@mdx-js/tag": "^0.20.3",
    "gatsby": "^2.32.13",
    "gatsby-image": "^2.0.34",
    "gatsby-link": "^2.0.16",
    "gatsby-mdx": "^0.4.4",
    "gatsby-plugin-manifest": "^2.0.24",
    "gatsby-plugin-nprogress": "^2.0.10",
    "gatsby-plugin-offline": "^2.0.25",
    "gatsby-plugin-react-helmet": "^3.0.11",
    "gatsby-plugin-sharp": "^2.0.31",
    "gatsby-plugin-styled-components": "^3.0.7",
    "gatsby-plugin-typography": "^2.2.10",
    "gatsby-remark-autolink-headers": "^2.0.16",
    "gatsby-remark-code-repls": "^2.0.7",
    "gatsby-remark-copy-linked-files": "^2.0.11",
    "gatsby-remark-images": "^3.0.10",
    "gatsby-remark-prismjs": "^3.2.6",
    "gatsby-source-filesystem": "^2.0.28",
    "gatsby-transformer-remark": "^2.3.8",
    "gatsby-transformer-sharp": "^2.1.17",
    "graphql": "^14.1.1",
    "lodash": "^4.17.11",
    "prismjs": "^1.16.0",
    "prop-types": "^15.7.2",
    "react": "^16.8.5",
    "react-dom": "^16.8.5",
    "react-helmet": "^5.2.0",
    "react-instagram-embed": "^1.5.0",
    "react-player": "^1.10.0",
    "react-tweet-embed": "^1.1.1",
    "react-typeform-embed": "^0.2.0",
    "react-typography": "^0.16.19",
    "styled-components": "^4.2.0",
    "typescript": "^3.3.4000",
    "typography": "^0.16.19",
    "typography-theme-doelger": "^0.16.19"
  },
  "devDependencies": {
    "eslint": "^5.15.3",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-prettier": "^4.1.0",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-react": "^7.12.4",
    "husky": "^1.3.1",
    "lint-staged": "^8.1.5",
    "now": "^14.2.3",
    "prettier": "^1.16.4"
  },
  "keywords": [
    "gatsby"
  ],
  "license": "MIT",
  "scripts": {
    "now-build": "npm run build",
    "deploy": "npx now switch swizec-react-dataviz && npx now",
    "build": "gatsby build",
    "develop": "gatsby develop",
    "format": "npx prettier --write ./**/*.{js,json,md,html,css}",
    "start": "npm run develop",
    "serve": "gatsby serve",
    "test": "echo \"Write tests! -> https://gatsby.dev/unit-testing\""
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/gatsbyjs/gatsby-starter-default"
  },
  "bugs": {
    "url": "https://github.com/gatsbyjs/gatsby/issues"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,json,md,html,css}": [
      "prettier --write",
      "git add"
    ],
    "*.js": [
      "eslint --fix",
      "git add"
    ]
  }
}
